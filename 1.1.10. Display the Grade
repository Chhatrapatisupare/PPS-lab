marks = []
for i in range(1, 6):
	mark = float(input(f"subject {i}: "))
	marks.append(mark)
average = sum(marks) / 5
grade = ""
if 90 <= average <= 100:
	grade = "A"
elif 80 <= average <= 89:
	grade = "B"
elif 70 <= average <= 79:
	grade = "C"
elif 60 <= average <= 69:
	grade = "D"
else:
	grade = "F"
print(f"Average Marks: {average:.2f}")
print(f"Grade: {grade}")
